<br />
<div class="booyah-box col-10 offset-1">
	<h1 class="page-header"><%= @artist.name %></h1>
	<% @artist.albums(country: 'US').each do |album| %>
	 <h3><%= album.name %></h3>
	 <ul>
	   <% album.tracks.each do |track| %>
	     <li>
	     	
	       <%= button_to 'Like', song_likes_path(@artist), method: :show %>
	       <%= track.name %>
	       

	     </li>
	   <% end %>
	 </ul>
	<% end %>
</div>	

	


